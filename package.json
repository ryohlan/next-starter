{
  "name": "next-starter",
  "version": "1.0.0",
  "description": "",
  "scripts": {
    "dev": "nodemon server/index.ts",
    "build": "next build && tsc --project tsconfig.server.json",
    "start": "NODE_ENV=production next build && tsc --project tsconfig.server.json && node production-server/server/index.js",
    "build-functions": "NODE_ENV=production next build && tsc --project tsconfig.firebase.json",
    "ts-watch": "tsc -w",
    "tsc": "tsc",
    "install-deps": "cp ./package* dist && cd dist && npm i",
    "cp-static": "cp -r static dist/static",
    "deploy": "npm run build-functions && npm run install-deps && firebase deploy",
    "docz:dev": "docz dev",
    "docz:build": "docz build"
  },
  "license": "ISC",
  "dependencies": {
    "@babel/runtime": "^7.0.0-beta.54",
    "@types/next": "^6.0.4",
    "@types/node": "^10.5.2",
    "@types/react": "^16.4.6",
    "@types/react-redux": "^6.0.4",
    "@zeit/next-typescript": "^1.1.0",
    "babel-plugin-module-resolver": "^3.1.1",
    "babel-plugin-styled-components": "^1.5.1",
    "babel-plugin-transform-runtime": "^6.23.0",
    "body-parser": "^1.18.3",
    "express": "^4.16.3",
    "firebase": "^5.3.0",
    "firebase-admin": "^5.13.0",
    "firebase-functions": "^1.1.0",
    "isomorphic-fetch": "^2.2.1",
    "next": "^6.1.1",
    "next-routes": "^1.4.2",
    "nodemon": "^1.18.3",
    "react": "^16.4.1",
    "react-dom": "^16.4.1",
    "react-redux": "^5.0.7",
    "recompose": "^0.27.1",
    "redux": "^4.0.0",
    "redux-devtools-extension": "^2.13.5",
    "redux-saga": "^0.16.0",
    "styled-components": "^3.3.3",
    "ts-node": "^7.0.0",
    "typescript": "^2.9.2",
    "typescript-fsa": "^3.0.0-beta-2",
    "universal-cookie": "^2.2.0",
    "webpack": "^4.16.1"
  },
  "devDependencies": {
    "@babel/plugin-transform-runtime": "^7.0.0-beta.54"
  }
}
